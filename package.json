{
  "name": "razoch",
  "version": "1.0.0",
  "description": "A React/Redux Playground",
  "main": "build/server/server.min",
  "scripts": {
    "------------------------ LifeCycle Scripts ------------------------": "",
    "prepush": "run-s lint:scss lint:code test:stat",
    "------------------------ Common Scripts ------------------------": "",
    "clean:all": "rimraf node_modules && npm install && rimraf build",
    "lint:scss": "stylelint './source/**/*.scss'",
    "lint:code": "eslint --ext .js,.jsx source webpack enzyme",
    "test:stat": "jest --silent --coverage=true",
    "------------------------ Development Scripts ------------------------": "",
    "build:dev": "webpack --display-error-details --config webpack/webpack.dev.babel.js",
    "start:dev": "cross-env NODE_ENV=development node build/server/server",
    "zboot:dev": "run-s clean:all build:dev start:dev",
    "------------------------ Production Scripts ------------------------": "",
    "build:prd": "webpack --display-error-details --config webpack/webpack.prd.babel.js",
    "start:prd": "cross-env NODE_ENV=production node build/server/server.min",
    "zboot:prd": "run-s clean:all build:prd start:prd"
  },
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aoch/razoch.git"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "react-dom",
    "redux",
    "redux-thunk",
    "redux-observable",
    "redux-saga",
    "babel",
    "webpack",
    "jest",
    "enzyme"
  ],
  "author": "Andrew Och <aoch@hotmail.co.uk>",
  "contributors": [
    "Asad Razvi <debugme@hotmail.com>"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/aoch/razoch/issues"
  },
  "homepage": "https://github.com/aoch/razoch#readme",
  "dependencies": {
    "express": "4.16.2",
    "prop-types": "15.6.1",
    "ramda": "0.25.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-observable": "0.18.0",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.2.0",
    "rxjs": "5.5.6",
    "spdy": "3.4.7",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "2.0.4",
    "webpack-hot-middleware": "2.21.1",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.10",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.18.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "3.0.4",
    "husky": "0.14.3",
    "jest": "22.4.2",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "stylelint": "9.1.1",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-standard": "18.2.0",
    "uglifyjs-webpack-plugin": "1.2.2",
    "webpack-node-externals": "1.6.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/build/testCoverage/",
    "collectCoverageFrom": [
      "source/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 4,
        "functions": 13,
        "lines": 5,
        "statements": 5
      }
    },
    "setupFiles": [
      "<rootDir>/enzyme/setUp.js"
    ]
  }
}
