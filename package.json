{
  "name": "razoch",
  "version": "1.0.0",
  "description": "A Todo web application",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf node_modules && npm install && rimraf build",
    "test": "jest",
    "prepush": "eslint --ext .js,.jsx source webpack enzyme",
    "build:dev": "webpack --display-error-details --config webpack/webpack.dev.babel.js",
    "build:prod": "webpack --display-error-details --config webpack/webpack.prod.babel.js",
    "run:dev": "cross-env NODE_ENV=development node build/server/server",
    "run:prod": "cross-env NODE_ENV=production node build/server/server.min",
    "all:dev": "run-s clean build:dev run:dev",
    "all:prod": "run-s clean build:prod run:prod"
  },
  "engines": {
    "node": "8.9.1",
    "npm": "5.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aoch/razoch.git"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "react-dom",
    "redux",
    "redux-thunk",
    "redux-observable",
    "redux-saga",
    "babel",
    "webpack",
    "jest",
    "enzyme"
  ],
  "author": "Andrew Och <aoch@hotmail.co.uk>",
  "contributors": [
    "Asad Razvi <debugme@hotmail.com>"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/aoch/razoch/issues"
  },
  "homepage": "https://github.com/aoch/razoch#readme",
  "dependencies": {
    "express": "4.16.2",
    "prop-types": "15.6.0",
    "ramda": "0.25.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-observable": "0.17.0",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.2.0",
    "rxjs": "5.5.6",
    "spdy": "3.4.7",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "2.0.4",
    "webpack-hot-middleware": "2.21.0",
    "webpack-node-externals": "1.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.7",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.13.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "jest": "22.0.4",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/build/testCoverage/",
    "collectCoverageFrom": [
      "source/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/enzyme/setUp.js"
    ]
  }
}
