{
  "name": "razoch",
  "version": "1.0.0",
  "description": "A React/Redux Playground",
  "scripts": {
    "------------------------ LifeCycle Scripts ------------------------": "",
    "prepush": "run-s lint:scss lint:code test:used test:stat test:dcap",
    "------------------------ Development Scripts -----------------------": "",
    "build:dev": "run-p build:client:dev build:http:server:dev",
    "start:dev": "run-p start:http:server:dev start:rest:server:dev",
    "zboot:dev": "run-s clean:dev build:dev start:dev",
    "------------------------ Production Scripts ------------------------": "",
    "build:prd": "run-p build:client:prd build:http:server:prd",
    "start:prd": "run-p start:http:server:prd start:rest:server:prd",
    "zboot:prd": "run-s clean:prd build:prd test:size start:prd",
    "------------------------ Maintenance Scripts ---------------------------": "",
    "clean:all": "rimraf build node_modules && npm install",
    "lint:scss": "stylelint --config config/stylelint/.rc './source/**/*.scss'",
    "lint:code": "eslint --ignore-path config/eslint/.ignore --config config/eslint/.rc --ext .js,.jsx source webpack config",
    "test:code": "jest --silent --coverage=false --config=/Users/andrew/git/projects/razoch/config/jest/config.json",
    "test:stat": "jest --silent --coverage=true --config=/Users/andrew/git/projects/razoch/config/jest/config.json",
    "test:dcap": "jscpd --config config/jspcd/config.yaml",
    "test:size": "bundlesize",
    "test:used": "testUsed",
    "------------------------ Development Helpers ------------------------": "",
    "clean:dev": "rimraf build/development",
    "build:client:dev": "env-cmd config/env/development/client.env webpack --config config/webpack/development/client.babel.js",
    "build:http:server:dev": "env-cmd config/env/development/http.server.env webpack --config config/webpack/development/http.server.babel.js",
    "start:http:server:dev": "env-cmd config/env/development/http.server.env node build/development/server/http.server",
    "start:rest:server:dev": "env-cmd config/env/development/rest.server.env swapi-server",
    "------------------------ Production Helpers ------------------------": "",
    "clean:prd": "rimraf build/production",
    "build:client:prd": "env-cmd config/env/production/client.env webpack --config config/webpack/production/client.babel.js",
    "build:http:server:prd": "env-cmd config/env/production/http.server.env webpack --config config/webpack/production/http.server.babel.js",
    "start:http:server:prd": "env-cmd config/env/production/http.server.env node build/production/server/http.server",
    "start:rest:server:prd": "env-cmd config/env/production/rest.server.env swapi-server"
  },
  "engines": {
    "node": "8.10.0",
    "npm": "5.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aoch/razoch.git"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "react-dom",
    "redux",
    "redux-thunk",
    "redux-observable",
    "redux-saga",
    "babel",
    "webpack",
    "jest",
    "enzyme"
  ],
  "author": "Andrew Och <aoch@hotmail.co.uk>",
  "contributors": [
    "Asad Razvi <debugme@hotmail.com>"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/aoch/razoch/issues"
  },
  "homepage": "https://github.com/aoch/razoch#readme",
  "dependencies": {
    "apicache": "1.2.1",
    "chalk": "2.3.2",
    "express": "4.16.3",
    "http-proxy-middleware": "0.18.0",
    "isomorphic-fetch": "2.2.1",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-observable": "0.18.0",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.2.0",
    "rxjs": "5.5.7",
    "serialize-javascript": "2.1.1",
    "spdy": "3.4.7",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "2.0.4",
    "webpack-hot-middleware": "2.21.2",
    "winston": "2.4.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "bundlesize": "0.16.0",
    "compression-webpack-plugin": "1.1.11",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.10",
    "env-cmd": "7.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "3.0.6",
    "husky": "0.14.3",
    "jest": "22.4.2",
    "jscpd": "^0.6.17",
    "node-sass": "4.7.2",
    "npm-check": "5.6.0",
    "npm-run-all": "4.1.2",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.7",
    "stylelint": "9.1.3",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-standard": "18.2.0",
    "swapi-server": "1.0.1",
    "test-used": "1.0.0",
    "uglifyjs-webpack-plugin": "1.2.3",
    "webpack-node-externals": "1.6.0"
  },
  "bundlesize": [
    {
      "path": "build/production/client/client.*.js",
      "maxSize": "30 kB"
    },
    {
      "path": "build/production/client/client.*.css",
      "maxSize": "1 kB"
    },
    {
      "path": "build/production/client/vendor.*.js",
      "maxSize": "100 kB"
    }
  ]
}
