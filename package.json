{
  "name": "razoch",
  "version": "1.0.0",
  "description": "A Todo web application",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build node_modules && npm install",
    "build": "webpack -p && babel source/server --out-dir build/server --config ./.babelrc && cp server.* build/server",
    "start": "node build/server/index",
    "shebang": "npm run clean && npm run build && npm run start"
  },
  "engines": {
    "node": "8.9.1",
    "npm": "5.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aoch/razoch.git"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "react-dom",
    "babel",
    "webpack"
  ],
  "author": "Andrew Och <aoch@hotmail.co.uk>",
  "contributors": [
    "Asad Razvi <debugme@hotmail.com>"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/aoch/razoch/issues"
  },
  "homepage": "https://github.com/aoch/razoch#readme",
  "dependencies": {
    "express": "4.16.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "spdy": "3.4.7"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "html-webpack-plugin": "2.30.1",
    "webpack": "3.10.0"
  }
}
