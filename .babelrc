{
	"presets": [
		"react",
		"env"
	],
	"plugins": [
		"transform-class-properties"
	],
	"env": {
		"production": {
			"plugins": [
				[
					"transform-react-remove-prop-types",
					{
						"mode": "wrap",
						"ignoreFilenames": [
							"node_modules"
						]
					}
				]
			]
		},
		"development": {
			"plugins": []
		}
	}
}
